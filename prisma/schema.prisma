generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model uploads {
  id       Int     @default(autoincrement()) @id
  path     String  @unique
  title    String
  user_uid String?
  users    users?  @relation(fields: [user_uid], references: [uid])
}

model users {
  email    String    @unique
  nickname String?   @default("")
  password String
  uid      String    @id
  uploads  uploads[]
}